# this is a system wide config file for git.
#  a '#' or ';' character indicates a comment
#  How to use it:
#    cp this file into $(prefix)/etc/gitconfig
#  or by hard or soft link
#    sudo ln <path>/sys_gitconfig /etc/gitconfig
#

[color]
    branch = true
    diff = true
    interactive = true
    status = true



